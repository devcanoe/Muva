{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": "standard-with-typescript",
  "overrides": [],
  "plugins": ["esbenp.prettier-vscode"],
  "parserOptions": {
    "ecmaVersion": "latest",
    "sourceType": "module"
  },
  "rules": {
    "no-unused-vars": "error",
    "no-unused-expressions": "error",
    "no-duplicate-imports": "error",
    "arrow-spacing": "warn",
    "camelcase": "warn",
    "eqeqeq": "error",
    "require-await": "error"
  }
}
